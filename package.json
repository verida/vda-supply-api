{
  "name": "@verida/vda-token-supply",
  "version": "0.0.1",
  "description": "VDA Token: Circulating Supply",
  "main": "dist/server.js",
  "scripts": {
    "dev": "yarn build; nodemon src/server.js",
    "build": "rm -rf dist && tsc && rm -rf dist/assets && cp -R assets dist/",
    "prestart": "yarn build",
    "start": "node dist/server.js",
    "tests": "ts-mocha './tests/**/*.ts'",
    "tests-report-json": "ts-mocha './test/**/*.ts' --reporter json > test-results.json",
    "test": "ts-mocha",
    "lambda-deploy": "rm -rf dist && tsc && yarn claudia create --region us-east-1 --handler lambda.handler --deploy-proxy-api --no-optional-dependencies",
    "lambda-update": "rm -rf dist && tsc && yarn claudia update",
    "lambda-pack": "rm -rf dist && tsc && yarn claudia pack --no-optional-dependencies"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/verida/vda-token-supply.git"
  },
  "author": "Verida",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/verida/vda-token-supply/issues"
  },
  "engines": {
    "node": ">=16.06"
  },
  "homepage": "https://github.com/verida/vda-token-supply#readme",
  "dependencies": {
    "aws-serverless-express": "^3.4.0",
    "axios": "^1.2.3",
    "body-parser": "^1.19.0",
    "cors": "^2.8.5",
    "csv-parser": "^3.0.0",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "lodash": "^4.17.21"
  },
  "devDependencies": {
    "@types/express": "^4.17.13",
    "@types/express-session": "1.17.0",
    "@types/mocha": "^9.1.0",
    "claudia": "^5.14.1",
    "mocha": "^9.2.1",
    "nodemon": "^2.0.14",
    "ts-mocha": "^9.0.2",
    "typescript": "4.6.3"
  }
}
